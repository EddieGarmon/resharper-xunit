<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>xunitcontrib</id>
    <title>xUnit.net Test Support</title>
    <version>2.0.0-alpha-20140502</version>
    <authors>Matt Ellis</authors>
    <owners>Matt Ellis</owners>
    <description>A unit test provider for xUnit.net. Discovers and runs xUnit.net tests. Includes annotations to aid ReSharper inspections and Live Templates to speed up inserting test methods and asserts</description>
    <summary>A unit test provider for xUnit.net</summary>
    <releaseNotes>&#8226; Initial support for xUnit.net 2</releaseNotes>
    <projectUrl>https://github.com/xunit/resharper-xunit</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/resharper-xunit/xunit2/license.txt</licenseUrl>
    <iconUrl>http://download-codeplex.sec.s-msft.com/Download?ProjectName=xunit&amp;DownloadId=662625</iconUrl>
    <copyright>Copyright 2014 Matt Ellis</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="ReSharper" version="[8.2,8.3)" />
    </dependencies>
    <tags>resharper unittest xunit</tags>
  </metadata>
  <files>
    <!-- 8.2 plugin -->
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunitcontrib.runner.resharper.provider.8.2.dll"
          target="ReSharper\v8.2\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunitcontrib.runner.resharper.runner.8.2.dll"
          target="ReSharper\v8.2\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.dll"
          target="ReSharper\v8.2\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.abstractions.dll"
          target="ReSharper\v8.2\plugins" />
    <file src="..\src\xunitcontrib.runner.resharper.provider\bin\Release\xunit.runner.utility.dll"
          target="ReSharper\v8.2\plugins" />

    <!-- Settings and annotations -->
    <file src="..\src\xunitcontrib.runner.resharper.provider\templates.dotSettings"
          target="ReSharper\vAny\settings" />
    <file src="..\ExternalAnnotations\xunit.xml"
          target="ReSharper\vAny\annotations" />
  </files>
</package>
